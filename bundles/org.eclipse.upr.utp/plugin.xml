<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated utp.profile -->
      <package
            uri="http://www.omg.org/spec/UTP/20120801/utp.xmi"
            class="org.eclipse.upr.utp.UTPPackage"
            genModel="model/utp.profile.genmodel"/>
